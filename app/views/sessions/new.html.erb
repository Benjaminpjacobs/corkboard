<div class="container">
  <div class="row">
    <div class="col-md-12 text-center">
      <h2>Welcome back</h1>
    </div>
  </div>
  <div class="form-signin login-session">
    <%= bootstrap_form_for(:session, url: login_path, method: :post) do |f| %>
      <%= f.text_field :email, placeholder: "Email", id: "email"%>
      <%= f.password_field :password, placeholder: "Password" %>
      <%= f.submit "Login", class: "btn login btn-primary" %>
    <% end %>
  </div>
</div>
