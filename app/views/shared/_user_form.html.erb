<%= bootstrap_form_for(@type, url: @path) do |f| %>
  <%= f.text_field :first_name, placeholder: "First Name", id: "first_name", value: user_form_first_name %><br>
  <%= f.text_field :last_name, placeholder: "Last Name", id: "last_name", value: user_form_last_name %><br>
  <%= f.text_field :zipcode, placeholder: 'Enter Zip Code', id: "zip_code"%><br>
  <%= f.text_field :email, placeholder: "Email", id: "email", value: user_form_email %><br>
  <%= f.password_field :password, placeholder: "Password" %><br>
  <%= f.password_field :password_confirmation, placeholder: "Confirm Password", label: "Confirm Password" %><br>
  <%= f.submit "Create Account", class: "btn login btn-primary" %>
<% end %>


