<div class="row new-project-bid">
  <h3>Project Details</h3>
  <h4>Service:</h4>
  <p><%= @project.service.name%></p>
  <h4>Status:</h4>
  <p><%= @project.status %></p>
  <h4>Zipcode:</h4>
  <p><%= @project.zipcode %></p>
  <h4>Recurring:</h4>
  <p><%= @project.recurring %></p>
  <h4>Description:</h4>
  <p><%= @project.description %></p>
  <h4>Created:</h4>
  <p><%= @project.created_at %></p>
  <h4>Timeline:</h4>
  <p><%= @project.timeline %></p>
  <h4>Requested By:</h4>
  <p><%= @project.requester.full_name%></p>
</div>
